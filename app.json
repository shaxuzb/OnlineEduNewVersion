{
  "expo": {
    "name": "MATH ME",
    "slug": "mathme",
    "version": "1.0.22",
    "orientation": "portrait",
    "icon": "./assets/images/icon.png",
    "scheme": "mathme",
    "platforms": ["ios", "android"],
    "userInterfaceStyle": "automatic",
    "splash": {
      "image": "./assets/images/logo.png",
      "dark": {
        "image": "./assets/images/logo.png",
        "backgroundColor": "#2A39C5",
        "resizeMode": "contain"
      },
      "resizeMode": "contain",
      "backgroundColor": "#2A39C5",
      "imageWidth": 150
    },
    "ios": {
      "supportsTablet": false,
      "bundleIdentifier": "com.rbsx.mathmeeducation",
      "infoPlist": {
        "ITSAppUsesNonExemptEncryption": false,
        "UIViewControllerBasedStatusBarAppearance": true
      }
    },
    "android": {
      "adaptiveIcon": {
        "foregroundImage": "./assets/images/adaptive-icon.png",
        "backgroundColor": "#ffffff"
      },
      "softwareKeyboardLayoutMode": "pan",
      "edgeToEdgeEnabled": true,
      "package": "com.anonymous.onlineedu"
    },
    "updates": {
      "enabled": true,
      "checkAutomatically": "ON_ERROR_RECOVERY",
      "fallbackToCacheTimeout": 0
    },
    "plugins": ["expo-dev-client", "expo-secure-store"],
    "extra": {
      "API_URL": "https://edu-api.crmuz.uz",
      "router": {},
      "eas": {
        "projectId": "34ba133e-e407-4f20-a0cc-cbd3d9e8a6fd"
      }
    }
  }
}
