{
  "expo": {
    "name": "MATH ME",
    "slug": "onlineedu",
    "version": "1.0.18",
    "orientation": "portrait",
    "icon": "./assets/images/icon.png",
    "scheme": "onlineedu",
    "platforms": ["ios", "android"],
    "userInterfaceStyle": "automatic",
    "splash": {
      "image": "./assets/images/logo.png",
      "dark": {
        "image": "./assets/images/logo.png",
        "backgroundColor": "#2A39C5",
        "resizeMode": "contain"
      },
      "resizeMode": "contain",
      "backgroundColor": "#2A39C5",
      "imageWidth": 150
    },
    "ios": {
      "supportsTablet": true,
      "bundleIdentifier": "com.onlineedu.app",
      "infoPlist": {
        "ITSAppUsesNonExemptEncryption": false
      }
    },
    "android": {
      "adaptiveIcon": {
        "foregroundImage": "./assets/images/adaptive-icon.png",
        "backgroundColor": "#ffffff"
      },
      "softwareKeyboardLayoutMode": "pan",
      "edgeToEdgeEnabled": true,
      "package": "com.anonymous.onlineedu"
    },
    "updates": {
      "enabled": true,
      "checkAutomatically": "ON_ERROR_RECOVERY",
      "fallbackToCacheTimeout": 0
    },
    "plugins": ["expo-dev-client", "expo-secure-store"],
    "extra": {
      "API_URL": "https://edu-api.crmuz.uz",
      "router": {},
      "eas": {
        "projectId": "84353f25-8841-4253-82ab-75e6d2701755"
      }
    }
  }
}
